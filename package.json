{
  "name": "abjadcompile",
  "main": "./lib/abjadcompile",
  "version": "0.1.1",
  "description": "A short description of your package",
  "keywords": [
  ],
  "repository": "https://github.com/jefftrevino/abjadcompile",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },

  "activationCommands": {
    "atom-workspace": "abjadcompile:lyToPdf"
  },

  "consumedServices": {
        "lilycompile": {
          "description": "renders editor's filepath from .ly to .pdf file in new editor pane",
          "versions": {
            "0.7.0": "consumeCompile"
          }
        },
        "hydrogen.provider": {
          "description": "grabs an instance of hydrogenProvider from Hydrogen",
          "versions": {
            "^1.2.0": "consumeHydrogen"
          }
        }
      },

  "dependencies": {
    "figlet": "1.0.8",
    "atom-package-deps": "^4.6.2"
  },

  "package-deps": [
    "lilycompile",
    "hydrogen",
    "atlilypond"
  ]
}
